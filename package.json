{
  "name": "eggAndWebpack",
  "version": "3.2.0",
  "description": "front side of kikuu",
  "scripts": {
    "build": "cross-env easywebpack build prod",
    "build-dev": "cross-env easywebpack build dev",
    "build-test": "cross-env UPLOAD_CDN=test easywebpack build test",
    "build-prod": "cross-env UPLOAD_CDN=prod easywebpack build prod",
    "start": "node index.js",
    "start-test": "cross-env EGG_SERVER_ENV=test PORT=3000 node index.js",
    "start-prod": "cross-env EGG_SERVER_ENV=prod NODE_ENV=production PORT=80 ENABLE_NODE_LOG=YES npm start",
    "kill": "pkill --signal SIGINT egg-vue-webpack-boilerplate",
    "kill-mac": "kill -9 $(lsof -i:7001 |grep 'node'|awk '{print $2}')",
    "kill-win": "taskkill -PID $(netstat -ano | findstr 7001|awk '{print $5}') -F",
    "kill-prod": "kill -9 $(lsof -i:80 |grep 'node'|awk '{print $2}')",
    "kill-dev": "kill -9 $(lsof -i:7001 |grep 'node'|awk '{print $2}')",
    "kill-test": "kill -9 $(lsof -i:3000 |grep 'node'|awk '{print $2}')",
    "restart": "npm run kill-port && npm run start",
    "lint": "eslint .",
    "fix": "eslint --fix ."
  },
